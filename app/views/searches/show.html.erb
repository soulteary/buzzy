<%= turbo_frame_tag "bar-content" do %>
  <% if @card %>
    <%= auto_submit_form_with url: card_path(@card), method: :get, data: { turbo_action: "advance", turbo_frame: "_top", search_redirect: true } %>
  <% else %>
    <%= render "results", search_results: @search_results %>
  <% end %>
<% end %>
